{
  "name": "WebView TTS Examples",
  "description": "Explore different approaches of using TTS inside WebView Tag",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "webview",
    "audio",
    "tts"
  ],
  "webview": {
    "partitions": [
      {
        "name": "main",
        "accessible_resources": ["www/*"]
      }
    ]
  }
}